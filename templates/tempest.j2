swift_reseller_admin_role = admin

[compute]
image_ref = {{ rally_cirros_image_uuid }}
image_ref_alt = {{ rally_cirros_image_uuid }}
flavor_ref = {{ rally_flavor_uuid }}
flavor_ref_alt = {{ rally_flavor_uuid }}
fixed_network_name = {{ rally_network_name }}

[object-storage]
